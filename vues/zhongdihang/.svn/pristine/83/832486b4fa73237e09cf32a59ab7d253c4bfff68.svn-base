package com.zhongdihang.bankdispatch.modular.service.dto;

import java.io.Serializable;
import java.util.List;

/**
 * @Author: Arthur Han
 * @Description:
 * @CreateTime: 2017/8/23 21:50
 * @Modified Byï¼š
 * @Version: V1.0.0
 */
public class AuthDto implements Serializable{
    private static final long serialVersionUID = -663173285862116728L;
    private String url;

    private List<String> roleName;

    private String method;

    public AuthDto() {
    }

    public AuthDto(String url, List<String> roleName, String method) {
        this.url = url;
        this.roleName = roleName;
        this.method = method;
    }

    public String getUrl() {
        return url;
    }

    public void setUrl(String url) {
        this.url = url;
    }

    public List<String> getRoleName() {
        return roleName;
    }

    public void setRoleName(List<String> roleName) {
        this.roleName = roleName;
    }

    public String getMethod() {
        return method;
    }

    public void setMethod(String method) {
        this.method = method;
    }
}
